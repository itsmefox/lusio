<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="lusio-beans" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/lusio</jta-data-source>
		<class>ch.viascom.lusio.entity.Account</class>
		<class>ch.viascom.lusio.entity.Account_log</class>
		<class>ch.viascom.lusio.entity.Field</class>
		<class>ch.viascom.lusio.entity.Game</class>
		<class>ch.viascom.lusio.entity.Session</class>
		<class>ch.viascom.lusio.entity.Tip</class>
		<class>ch.viascom.lusio.entity.User</class>
	</persistence-unit>
</persistence>